

<form method="post" enctype="multipart/form-data" asp-controller="Home" asp-action="FileUpload">

   

    <div class="mb-3">
       
                <label class="form-label">upload file</label>
                <input type="file" name="file" />

            </div> 
            <div class="mb-3">
                <input type="submit" value="upload file"/>

